<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Selenium_Framework_Automation_Suite" verbose="1">
    <groups>
        <run>
            <include name="ErrorHandling"/>
        </run>
    </groups>
    <test thread-count="5" name="Error Validations">
        <classes>
            <class name="com.lhr.selenium.tests.ui.ErrorValidationsTest"/>
        </classes>
    </test>
</suite>